<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="0"
    android:pivotX="50%"
    android:pivotY="50%"
    android:toDegrees="360">
    <shape
        android:innerRadiusRatio="3"
        android:shape="ring"
        android:useLevel="false">
        <gradient
            android:centerColor="@color/common_progressbar_centerColor"
            android:endColor="@color/common_progressbar_endColor"
            android:startColor="@color/common_progressbar_startColor"
            android:type="sweep" />
    </shape>

</rotate>


